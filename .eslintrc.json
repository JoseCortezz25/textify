{
  "extends": "next/core-web-vitals",
  "rules": {
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "semi": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "camelcase": [
      "error",
      {
        "allow": [
          "api_url",
          "other_identifier"
        ]
      }
    ],
    "no-unused-vars": "off",
    "comma-dangle": [
      "error",
      {
        "functions": "never"
      }
    ],
    "no-debugger": "off"
  }
}